<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="site/inthebus/database/db.properties"></properties>

<typeAliases>
	<typeAlias type="site.inthebus.model.TestMemberDTO" alias="TestMemberDTO"/>
	<typeAlias type="site.inthebus.model.TestBookmarkDTO" alias="TestBookmarkDTO"/>
</typeAliases>

  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  
  <mappers>
  	<mapper resource="site/inthebus/mapper/TestMemberMapper.xml"/>
  	<mapper resource="site/inthebus/mapper/TestBookmarkMapper.xml"/>
  </mappers>
</configuration>